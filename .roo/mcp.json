{
  "mcpServers": {
    "g-search": {
      "command": "npx",
      "args": [
        "-y",
        "g-search-mcp"
      ],
      "alwaysAllow": [
        "search"
      ]
    },
    "context7-mcp": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@upstash/context7-mcp",
        "--key",
        "3d4f4f3a-0be7-40fd-b26e-485ca312dc1c"
      ],
      "alwaysAllow": [
        "resolve-library-id",
        "get-library-docs"
      ],
      "timeout": 30,
      "disabled": false
    },
    "duckduckgo-mcp-server": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@nickclyde/duckduckgo-mcp-server",
        "--key",
        "3d4f4f3a-0be7-40fd-b26e-485ca312dc1c"
      ],
      "alwaysAllow": [
        "fetch_content",
        "search"
      ],
      "disabled": false,
      "timeout": 30
    },
    "tavily-mcp": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@tavily-ai/tavily-mcp",
        "--key",
        "3d4f4f3a-0be7-40fd-b26e-485ca312dc1c",
        "--profile",
        "wasteful-porpoise-FzeEwD"
      ],
      "alwaysAllow": [
        "tavily-extract",
        "tavily-search</tool_tool_code>\n<arguments>\n{\n  \"query\": \"Lai Ch√¢u province Vietnam history culture food getting there\",\n  \"search_depth\": \"advanced\",\n  \"max_results\": 5\n}\n</arguments>\n</use_mcp_tool>",
        "tavily-search"
      ],
      "disabled": false,
      "timeout": 30
    },
    "DeepResearchMCP": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@ameeralns/DeepResearchMCP",
        "--key",
        "3d4f4f3a-0be7-40fd-b26e-485ca312dc1c"
      ],
      "alwaysAllow": [
        "complete-research",
        "generate-report",
        "execute-research-step",
        "initialize-research"
      ],
      "timeout": 1800
    }
  }
}